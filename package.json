    "next": "^14.1.4",
    "next-auth": "^4.24.7",
    "next-international": "^1.2.4",
    "next-seo": "^6.5.0",
    "nodemailer": "^6.9.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.3",
    "react-icons": "^4.12.0",
    "react-select": "^5.8.0",
    "react-toastify": "^10.0.4",
    "react-top-loading-bar": "^2.3.1",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
